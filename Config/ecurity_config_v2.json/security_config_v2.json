{
  "encryption": {
    "enabled": true,
    "algorithm": "AES-256",
    "key": "your_encryption_key_here"
  },
  "token_validation": {
    "enabled": true,
    "expiration_time": 3600,
    "issuer": "ZB56"
  },
  "access_control": {
    "roles": [
      {
        "role": "admin",
        "permissions": ["read", "write", "delete"]
      },
      {
        "role": "user",
        "permissions": ["read"]
      }
    ],
    "default_role": "user"
  },
  "audit_logs": {
    "enabled": true,
    "log_file": "logs/access_logs.json"
  }
}
